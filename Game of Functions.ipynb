   "source": [
    "sample = input(\"Give input separated by comma:\").split(',')\n",
    "def sum_of_list(order):\n",
    "    total = 0\n",
    "    for i in order:\n",
    "        num = int(i)\n",
    "        total += num\n",
    "    return total\n",
    "\n",
    "print(f\"Result = {sum_of_list(sample)}\")"
   ]
