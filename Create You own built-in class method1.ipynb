    "class Power:\n",
    "    \n",
    "    def __init__(self,x,n):\n",
    "        self.x = x\n",
    "        self.n = n\n",
    "        \n",
    "    def raise_power(self):\n",
    "        return self.x ** self.n\n",
    "    \n",
    "num = int(input(\"x : \"))\n",
    "p = int(input(\"n : \"))\n",
    "\n",
    "my_class = Power(x=num, n=p)\n",
    "print(f\"Result :{my_class.raise_power()}\")"
   ]
